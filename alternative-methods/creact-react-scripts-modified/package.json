    "yargs-parser": "^18.1.2",
    "http-proxy": "^1.18.1",
    "chokidar": "^3.4.1"
  },
  "dependencies": {
    "@react-pdf/renderer": "^1.6.11",
    "hookrouter": "^1.2.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "simple-widgets": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.5",
    "@types/react": "^16.9.49",
    "acorn": "^8.0.1",
    "core-js": "^3.6.5",
    "rewire": "^5.0.0",
    "source-map-explorer": "^2.5.0",
    "typescript": "^4.0.3"
  },
  "browserslist": {
