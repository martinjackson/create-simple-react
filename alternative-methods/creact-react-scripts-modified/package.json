{
  "name": "isswor-reports",
  "version": "0.9.0",
  "description": "Reports for ISSWOR (instrument Repair)",
  "main": "index.js",
  "proxy": "https://127.0.0.1:4000/",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "set HTTPS=true && react-scripts start",
    "build:minified": "react-scripts build",
    "build": "node build.js",
    "test": "react-scripts test",
    "analyze": "source-map-explorer 'build/*.js'",
    "server": "cd ../server ; ./start.sh"
  },
  "keywords": [
    "React",
    "React-pdf.org",
    "ISSWOR"
  ],
  "author": "Martin A. Jackson",
  "license": "ISC",
  "repository": "https://ncsvmgitlab.fda.gov/martinjackson/isswor-reports.git",
  "resolutions": {
